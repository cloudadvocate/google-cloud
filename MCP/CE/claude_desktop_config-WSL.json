{
  "mcpServers": {
    "mcp-server-kubernetes": {
      "command": "wsl",
      "args": ["/usr/bin/node", "/home/gkrishna25/mcp/mcp-server-kubernetes/dist/index.js"]
    },
    "google-compute": {
      "command": "wsl",
      "args": [
        "bash",
        "-lc",
        "source /mnt/c/Users/GK-PC/gcloud-mcp-env.sh && source ~/.nvm/nvm.sh && nvm use 24 >/dev/null && npx -y @google-cloud/gcloud-mcp@latest"
