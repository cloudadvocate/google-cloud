        "GOOGLE_CLOUD_PROJECT": "GCP-PROJECT",
        "CLOUDSDK_CORE_PROJECT": "GCP-PROJECT",
        "GOOGLE_APPLICATION_CREDENTIALS": "PATH-OF-SVC-ACCOUNT/SAMPLE.json",
        "CLOUDSDK_CONFIG": "/home/gkrishna25/.config/gcloud-mcp",
        "CLOUDSDK_AUTH_CREDENTIAL_FILE_OVERRIDE": "PATH-OF-SVC-ACCOUNT/SAMPLE.json",
        "PATH": "/usr/bin:/bin"
      }
